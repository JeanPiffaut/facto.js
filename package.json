{
    "name": "project-template",
    "version": "1.0.0",
    "private": true,
    "dependencies": {
        "@fontsource/montserrat": "^5.0.18",
        "@fortawesome/fontawesome-free": "^6.5.2",
        "@testing-library/jest-dom": "^6.4.2",
        "@testing-library/react": "^15.0.5",
        "@testing-library/user-event": "^14.5.2",
        "bootstrap": "^5.3.3",
        "env-cmd": "^10.1.0",
        "firebase": "^10.12.0",
        "lazysizes": "^5.3.2",
        "prop-types": "^15.8.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
